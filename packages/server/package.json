{
    "name": "@yshvydak/test-dashboard-server",
    "version": "1.3.2",
    "description": "Backend server for yshvydak-test-dashboard",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && npm run copy-files",
        "copy-files": "mkdir -p dist/database && cp -r src/database/schema.sql dist/database/",
        "dev": "tsx watch src/index.ts",
        "test": "npx vitest run",
        "test:watch": "npx vitest",
        "test:ui": "npx vitest --ui",
        "test:coverage": "npx vitest run --coverage",
        "type-check": "tsc --noEmit",
        "start": "node dist/index.js",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix"
    },
    "keywords": [
        "playwright",
        "testing",
        "server",
        "api"
    ],
    "author": "Yurii Shvydak <y.shvydak@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.18.0",
        "fast-jwt": "^6.0.2",
        "sqlite3": "^5.1.6",
        "uuid": "^9.0.1",
        "ws": "^8.14.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.0",
        "@types/express": "^4.17.0",
        "@types/node": "^20.0.0",
        "@types/sqlite3": "^3.1.8",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "supertest": "^7.1.4",
        "tsx": "^4.0.0",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    }
}
